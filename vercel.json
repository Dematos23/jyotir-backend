{
    "version": 2,
    "routes": [
      { "src": "/(.*)", "dest": "src/server.js" }
    ],
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb",
          "includeFiles": ["prisma/**"]
        }
      }
    ],
    "build": {
      "env": {
        "DATABASE_URL": "@your-database-url"
      },
      "commands": [
        "prisma generate"
      ]
    }
  }
  